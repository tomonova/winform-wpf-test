<Window 
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    x:Class="WPFProjektv1.DialogBox"
    xmlns:local="clr-namespace:WPFProjektv1"
    Title="Exit"
    Height="190"
    Width="300"
    MinHeight="10"
    MinWidth="300"
    ResizeMode="CanResizeWithGrip"
    ShowInTaskbar="False"
    WindowStartupLocation="CenterOwner">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Button Name="okButton" IsDefault="True" Grid.Column="0" Margin="30,50" Click="okButton_Click">OK</Button>
        <Button Name="cancelButton" IsCancel="True" Grid.Column="3" Margin="30,50" Click="cancelButton_Click">Cancel</Button>
    </Grid >
</Window>